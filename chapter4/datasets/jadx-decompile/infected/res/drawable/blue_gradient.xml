<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff449def" />
            <stroke android:width="1dp" android:color="#ff2f6699" />
            <corners android:radius="5dp" />
            <padding android:left="10dp" android:top="5dp" android:right="5dp" android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff449def" android:endColor="#ff2f6699" android:angle="270.0" />
            <stroke android:width="1dp" android:color="#ff2f6699" />
            <corners android:radius="5dp" />
            <padding android:left="10dp" android:top="5dp" android:right="5dp" android:bottom="10dp" />
        </shape>
    </item>
</selector>
